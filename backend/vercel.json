{
    "version": 2,
    "builds": [
      {
        "src": "api/server.js",
        "use": "@vercel/node",
        "config": {
          "includeFiles": [
            "backend/config/**",
            "backend/controllers/**",
            "backend/middleware/**",
            "backend/models/**",
            "backend/routes/**",
            ".env"
          ]
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "api/server.js"
      }
    ]
  }
  